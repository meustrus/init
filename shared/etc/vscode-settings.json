{
  "breadcrumbs.enabled": true,
  "diffEditor.renderSideBySide": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "editor.acceptSuggestionOnEnter": "off",
  "editor.autoClosingBrackets": "never",
  "editor.autoIndent": "brackets",
  "editor.dragAndDrop": false,
  "editor.fontFamily": "Source Code Pro, Consolas, monospace",
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": true,
  "editor.renderFinalNewline": true,
  "editor.renderWhitespace": "boundary",
  "editor.rulers": [
    72,
    80,
    100,
    120
  ],
  "editor.suggestOnTriggerCharacters": false,
  "editor.suggestSelection": "first",
  "editor.tabCompletion": "on",
  "editor.wordBasedSuggestions": false,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "explorer.autoReveal": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.openEditors.visible": 0,
  "extensions.ignoreRecommendations": true,
  "gitlens.codeLens.enabled": false,
  "go.toolsManagement.autoUpdate": true,
  "java.completion.importOrder": [
    "",
    "javax",
    "java",
    "#"
  ],
  "java.configuration.checkProjectSettingsExclusions": false,
  "java.configuration.updateBuildConfiguration": "automatic",
  "java.errors.incompleteClasspath.severity": "ignore",
  "java.format.settings.profile": "onap-java-formatter",
  "java.format.settings.url": "https://gerrit.onap.org/r/gitweb?p=aai/aai-common.git;a=blob_plain;f=onap-java-formatter.xml;hb=refs/heads/master",
  "java.semanticHighlighting.enabled": true,
  "redhat.telemetry.enabled": false,
  "spellright.notificationClass": "information",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.scrollback": 10000,
  "vsicons.dontShowNewVersionMessage": true,
  "window.title": "${rootName}${separator}${dirty}${activeEditorShort}${separator}${appName}",
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.untitled.hint": "hidden",
  "workbench.iconTheme": "vscode-icons",
  "workbench.sideBar.location": "right",
  "workbench.startupEditor": "none",
  "[csharp]": {
    "editor.tabSize": 4
  },
  "[go]": {
    "editor.formatOnSave": false,
    "editor.insertSpaces": false,
    "editor.tabSize": 4
  },
  "[groovy]": {
    "editor.tabSize": 2
  },
  "[html]": {
    "editor.tabSize": 2
  },
  "[java]": {
    "editor.tabSize": 4
  },
  "[javascript]": {
    "editor.tabSize": 2
  },
  "[json]": {
    "editor.tabSize": 2
  },
  "[markdown]": {
    "editor.tabSize": 2
  },
  "[plaintext]": {
    "editor.wordWrap": "on"
  },
  "[python]": {
    "editor.tabSize": 4
  },
  "[ruby]": {
    "editor.tabSize": 2
  },
  "[terraform]": {
    "editor.tabSize": 2,
    "editor.wordWrap": "on"
  },
  "[sbt]": {
    "editor.tabSize": 2
  },
  "[scala]": {
    "editor.tabSize": 2
  },
  "[sql]": {
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.tabSize": 2
  },
  "[yaml]": {
    "editor.tabSize": 2
  }
}
